/* components/filter/filter.wxss */
page{
  width: 100%;
  height: 100%;
}
.nav_all {
  width: 100%;
  height: 100%;
}
.nav{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
}
.nav_item{
  width: 50%;
  height: 50%;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border: gray solid  1px;
  border-radius: 60rpx;
  background-color:#658089;
}

.false{
  color: #658089;
  height: 100%;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  border-radius: 60rpx;
  font-weight: bold;
}
.true{
  color: white;
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: space-around;
  font-weight: bold;
}