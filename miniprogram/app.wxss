/**app.wxss**/

@import '/miniprogram_npm/weui-miniprogram/weui-wxss/dist/style/weui.wxss';
@import '/assets/css/color.wxss';
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    position: relative;
}

button {
    background: initial;
}

button:focus {
    outline: 0;
}

button::after {
    border: none;
}

page {
    height: 100%;
    background: #f6f6f6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}